{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Register soccer player in European from the world",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/Yongjun1998/vega_lite/main/js/world.geo.json",
    "format": {"type": "topojson", "feature": "world"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Yongjun1998/vega_lite/main/data/FullData.csv"
        },
        "key": "country",
        "fields": ["number"]
      }
    },
    {"calculate": "datum.number", "as": "numbers"}
  ],
  "projection": {"type": "equirectangular"},
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "numbers",
      "type": "quantitative",
      "scale": {
        "type": "threshold",
        "domain": [200, 500, 1000],
        "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
      }
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "country"},
      {"field": "number", "type": "quantitative"}
    ]
  },
  "config": {}
}